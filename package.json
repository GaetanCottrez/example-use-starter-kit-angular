{
  "name": "storefront-phildar",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl --ssl-cert \"./secrets/local-cert.pem\"  --ssl-key \"./secrets/local-key.pem\" ",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "api": "json-server ./api/data.json -p 5050 -w",
    "doc:dashboard": "compodoc -p ./tsconfig.doc.json -s",
    "dev:ssr": "ng run storefront-phildar:serve-ssr",
    "serve:ssr": "node dist/storefront-phildar/server/main.js",
    "build:ssr": "ng build && ng run storefront-phildar:server",
    "prerender": "ng run storefront-phildar:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.1",
    "@angular/common": "~12.0.1",
    "@angular/compiler": "~12.0.1",
    "@angular/core": "~12.0.1",
    "@angular/forms": "~12.0.1",
    "@angular/localize": "~12.0.1",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/platform-server": "~12.0.1",
    "@angular/router": "~12.0.1",
    "@compodoc/compodoc": "^1.0.9",
    "@moltin/sdk": "6.3.0",
    "@ng-bootstrap/ng-bootstrap": "^9.1.1",
    "@nguniversal/express-engine": "^12.0.2",
    "bootstrap": "^4.5.0",
    "constants": "0.0.2",
    "express": "^4.15.2",
    "path-browserify": "^1.0.1",
    "rxjs": "~6.6.0",
    "stream": "0.0.2",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.1",
    "@angular/cli": "~12.0.1",
    "@angular/compiler-cli": "~12.0.1",
    "@nguniversal/builders": "^12.0.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.7.0",
    "json-server": "^0.16.3",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.1.4",
    "typescript": "~4.2.3"
  }
}
